{
	"$schema": "http://json.schemastore.org/launchsettings.json",
	"profiles": {
		"REST API (HTTP/1)": {
			"commandName": "Project",
			"dotnetRunMessages": true,
			"launchBrowser": false,
			"applicationUrl": "http://localhost:5001",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development",
				"ASPNETCORE_Kestrel__Endpoints__Http__Protocols": "Http1"
			}
		},
		"gRPC (HTTP/2)": {
			"commandName": "Project",
			"dotnetRunMessages": true,
			"launchBrowser": false,
			"applicationUrl": "https://localhost:5101",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development",
				"ASPNETCORE_Kestrel__Endpoints__Https__Protocols": "Http1AndHttp2"
			}
		},
		"Dual Mode": {
			"commandName": "Project",
			"dotnetRunMessages": true,
			"launchBrowser": true,
			"launchUrl": "swagger",
			"applicationUrl": "http://localhost:5001;https://localhost:5101",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development",
				"ASPNETCORE_Kestrel__Endpoints__Http__Protocols": "Http1",
				"ASPNETCORE_Kestrel__Endpoints__Https__Protocols": "Http1AndHttp2"
			}
		}
	}
}